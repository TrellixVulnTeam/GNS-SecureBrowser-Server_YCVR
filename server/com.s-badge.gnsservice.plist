<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN"
    "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>KeepAlive</key>
		<true/>
    <key>Label</key>
    <string>com.s-badge.gnsservice</string>
    <key>ServiceDescription</key>
    <string>GNS Badge service</string>
    <key>ProgramArguments</key>
    <array>             
        <string>/Library/GNS/gnsdeviceserver</string>
    </array>
    <key>RunAtLoad</key>
    <false/>
    <key>StandardErrorPath</key>
    <string>/Library/GNS/logs/gnsdeviceserver.err</string>
</dict>
</plist>